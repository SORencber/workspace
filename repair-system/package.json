{"name": "repair-management-system", "version": "1.0.0", "description": "Application for repair management system", "main": "server/server.js", "scripts": {"server": "node --max-old-space-size=2048 server/server.js", "client": "cd client && npm run dev", "start": "concurrently \"npm run server\" \"npm run client\"", "install-all": "npm install && cd client && npm install", "build": "cd client && npm run build", "debug": "concurrently -n \"client,server\" \"npm run client\" \"cross-env NODE_OPTIONS=--inspect-brk=9229 npm run server\""}, "dependencies": {"concurrently": "^8.2.2"}, "devDependencies": {"cross-env": "^7.0.3"}}